{
  "include": ["build.config.ts", "src"],
  "compilerOptions": {
    "jsx": "react",
    "outDir": "dist",
    "target": "ES2022",
    "module": "esnext",
    "moduleResolution": "bundler",
    "strict": true,
    "skipLibCheck": true,
    "declaration": false,
    "sourceMap": false,
    "noUnusedLocals": true,
    "esModuleInterop": true,
    "allowImportingTsExtensions": true
  }
}
